<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.diagnostics>
    <trace autoflush="true">
      <listeners>
        <add name="TextWriter" type="System.Diagnostics.TextWriterTraceListener" initializeData="trace.log" />
        <add name="ConsoleWriter" type="System.Diagnostics.ConsoleTraceListener" />
      </listeners>
    </trace>
  </system.diagnostics>
  <appSettings>
    <!-- Linux implementation -->
    <add key="DefaultEmulator" value="/home/meanbunny/projects/sdk/android-sdk-linux/tools/emulator" />
    <add key="DefaultEmulatorArguments" value="-avd {avdname} -scale .3" />
    <add key="DefaultItemFolder" value="emulators" />
  </appSettings>
</configuration>